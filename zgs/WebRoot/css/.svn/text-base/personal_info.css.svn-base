p.title{
	font-weight: bold;
	font-size: 40px;
	text-align:center;
}
.head_foot_content{
	width: 960px;
	margin: 0 auto;
}
.user{
	width:800px;
	margin: 0 auto;
	margin-top: 30px;
	margin-bottom: 85px;
}
.user_id{
	width: 500px;
	margin:0 auto;
	clear: both;

}
.user_id p{
	float: left;
	padding-top: 10px;
	color:#007310;
}
.user_input{
	float:right;
    width:390px;
	height: 40px;
    border-width: 1px;
    border-style: solid;
    border-color: #DFDFDF;
    border-radius:3px;
}
.user_id span{
	float: left;
	padding-top: 10px;
	margin-left: 90px;

}
.text{
	clear: both;
	width: 300px;
	margin: 0 auto;

}
.level{
	margin:50px;
	margin-left: 90px;
	margin-top: 0px;
}

.submit_btn{
	clear: left;
	background-color: #dddddd;
	padding:10px 30px;
	margin-top: 100px;
	
    border-radius: 3px;
	border-color: #dddddd;
	border-width: 0px;
}

