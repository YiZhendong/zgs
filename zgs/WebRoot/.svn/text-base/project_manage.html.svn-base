      </table>
    </div>
    
    <!-- 添加项目 -->
    <div class="inner" >
      <p class="content_title">申请新项目</p>
      
      <form>
      <div class="project_header">
	    <p class="item_name">项目名称</p>
		<input type="text" class="pname" placeholder="请输入项目名称" size="60">
		<input type="button" class="test" value="重复检测">
      </div>
      <div class="project_add">
        <p class="item_name">项目储备信息</p>
        <table class="mytable">
          <tr>
            <td class="td">项目类型：</td>
            <td class="td"><select >
        	<option value="null">（未选择）</option>
            <option value="1">类型1</option>
            <option value="2">类型2</option>
            </select></td>
            <td>项目金额：</td>
            <td><input type="text">（万元）</td>
          </tr>
        
       	  <tr>
            <td>完成情况：</td>
            <td colspan="3"><textarea></textarea></td>
          </tr>
        
          <tr>
            <td>项目简介：</td>
            <td><input type="text"></td>
          </tr>
          <tr>
            <td>建设地点：</td>
            <td><input type="text"></td>
          </tr>
        
          <tr>
            <td>开始时间：</td>
            <td><input class="easyui-datebox" style="width:150px;height:30px;"></input></td>
            <td>结束时间：</td>
            <td><input class="easyui-datebox" style="width:150px;height:30px;"></input></td>
          </tr>
        
          <tr>
            <td>项目内容：</td>
            <td colspan="3"><textarea></textarea></td> 
          </tr>
       
          <tr>
            <td>项目成果概述：</td>
            <td colspan="3" class="td"><textarea></textarea></td>
          </tr>
        
          <tr>
            <td>风险评估：</td>
            <td colspan="3"><textarea></textarea></td>
          </tr><br/>
      </table>
      <input type="submit" class="submit_btn" value="提交">
      
	</div>
    </form>
    </div>
    
    <!-- 项目审批 -->
